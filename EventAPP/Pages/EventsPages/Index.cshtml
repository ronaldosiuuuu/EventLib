@page
@model EventAPP.Pages.EventsPages.IndexModel
@{
}
<h1>Liste over eventsne</h1>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>EventSlags</th>
            <th>Description</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var d in Model.events)
        {
            <td>@d.Id</td>
            <td>@d.Name</td>
            <td>@d.EventSlags</td>
            <td>@d.Description</td>
            <td>@d.Date</td>
        }
    </tbody>
</table>

