@page
@model EventAPP.Pages.Events.IndexModel
@{
}
<h1>Liste over eventsne</h1>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>EventSlags</th>
            <th>Description</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var d in Model.Events)
        <tr>
            <td>@d.Id</td>
            <td>@d.Name</td>
        </tr>
    </tbody>
</table>
