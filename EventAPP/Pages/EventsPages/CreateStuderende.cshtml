@page
@model EventAPP.Pages.EventsPages.CreateStuderendeModel
@{
}
<h1> Create a new User</h1>
<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div class="form-group">
                <label asp-for="@Model.Email" class="control-label">Email</label><br>
                <input asp-for="@Model.Email" class="form-control" /><br>
                <span asp-validation-for="@Model.Email" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input asp-for="@Model.Password" class="form-control" /><br />
                <span asp-validation-for="@Model.Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label>Admin</label><br>
                <select asp-for="Users" class="form-control">
                    @foreach (var pt in Model.Users)
                    {
                        <option value="@pt">@pt</option>
                    }
                </select>
            </div>

            <div class="form-group">
                <input type="submit" value="Opret" />
            </div>

        </form>
    </div>
</div>